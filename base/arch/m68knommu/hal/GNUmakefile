	mostlyclean-compile mostlyclean-generic mostlyclean-libtool \
	pdf pdf-am ps ps-am tags uninstall uninstall-am


rtai_hal.ko: src2obj = $(patsubst %.$(1),%.o,$(filter %.$(1),$(call notdir,$(2))))
rtai_hal.ko: $(libhal_a_SOURCES)
	@if test \! -r Module.symvers ; then $(LN_S) -f "`cd $(top_builddir) && pwd`/Module.symvers" . ; fi; \
	for src in $^; do if test \! -r `basename $$src`; then $(LN_S) $$src; fi; done; 		    rm -f /usr/src/linux-2.6.24/.tmp_versions/rtai_*.mod /usr/src/linux-2.6.24/.tmp_versions/*_rt.mod;                     $(MAKE) -C /usr/src/linux-2.6.24 CROSS_COMPILE=$(CROSS_COMPILE) CC=gcc ARCH=i386 		    SUBDIRS=$$PWD M=$$PWD rtai_srctree=`cd $(top_srcdir) && pwd` rtai_srcdir=`cd $(srcdir) && pwd` V=$(V) modules 		    rtai_objs='$(call src2obj,c,$^) $(call src2obj,S,$^) $(call src2obj,cc,$^) $(call src2obj,cpp,$^)' rtai_target=$(basename $@) \
	

