vpath %.c @RTAI_INSTALLDIR@/source/skins/rtai

CC = $(top_builddir)/gcic/gcic

override CFLAGS = -g

AM_CPPFLAGS = \
	-D__IN_RTAI__ \
	--gcic-backend=$(prefix)/libexec/gcic \
	--skin-code \
	-I$(srcdir)/.. \
	-I$(top_srcdir)/include \
	-I@RTAI_INSTALLDIR@/source/skins \
	-I@RTAI_INSTALLDIR@/source/include

lib_LIBRARIES = librtai_s.a

nodist_librtai_s_a_SOURCES = \
	module.c \
	registry.c \
	task.c \
	timer.c \
	sem.c \
	event.c \
	mutex.c \
	cond.c \
	queue.c \
	heap.c \
	alarm.c
