imgXPM.o:	$(srcdir)/imgXPM.c
	$(COMPILE) -c `@CYGPATH@ $(srcdir)/imgXPM.c` -o $@

imgCompound.o:	$(srcdir)/imgCompound.c
	$(COMPILE) -c `@CYGPATH@ $(srcdir)/imgCompound.c` -o $@


